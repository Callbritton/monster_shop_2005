<h1> Please Fill In Each Form</h1>

<%= form_tag '/profile', method: :update do %>
  <%= label_tag :name %>
  <%= text_field_tag :name, @user.name %>
  <%= label_tag :address%>
  <%= text_field_tag :address, @user.address %>
  <%= label_tag :city %>
  <%= text_field_tag :city, @user.city %>
  <%= label_tag :state %>
  <%= text_field_tag :state, @user.state %>
  <%= label_tag :zip %>
  <%= text_field_tag :zip, @user.zip %>
  <%= submit_tag "Submit"%>
<% end %>
